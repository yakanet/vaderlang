Point :: struct {
    x: u32
    y: u32
}

dot:: fn(this: Point, other: Point) -> u32 {
    return (this.x * other.x) + (this.y * other.y)
}

plus1 :: fn(this: u32) -> u32 {
    return this + 1
}

main :: fn() -> u32 {
    p1 :: Point { 2, 3 }
    p2 :: Point { 4, 5 }
    return p1.dot(p2).plus1()
}