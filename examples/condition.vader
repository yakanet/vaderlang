@intrinsic
print :: fn(message: string) -> void

main :: fn() -> u32 {
    x :: 32
    // Simple if statement
    if x == 32 {
        print("equals\n")
    }

    // Simple if statement with parentheses
    if (x == 32) {
        print("second equals\n")
    }

    // If with else block
    if x != 32 {
        print("equals\n")
    } else {
        print("not equals\n")
    }
    return 0
}