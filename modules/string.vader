String :: struct {
    length: u32
    value: u8[]
}

equals :: fn(this: String, other: String) : boolean {
    if(this.length != other.length) {
        return false
    }

    // TODO use a memory comparaison instead of for loop
    for (i := 0; i<this.length; i++ ) {
        if(this.value[i] != other.value[i]) {
            return false
        }
    }
    return true
}

