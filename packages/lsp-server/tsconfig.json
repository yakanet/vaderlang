{
  "compilerOptions": {
    "lib": [
      "ES2020"
    ],
    "module": "commonjs",
    "target": "es2020",
    "sourceMap": true,
    "allowJs": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noPropertyAccessFromIndexSignature": false
  }
}
